---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "11",
      "tags"     : [{% for tag in post.tags %}"{{ tag }}",{% endfor %} null],
      "href"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : {
        "day": "{{ post.date | date: "%d" }}",
        "month": "{{ post.date | date: "%B" }}",
        "year": "{{ post.date | date: "%Y" }}"
      }
    },
  {% endfor %}
        null
]